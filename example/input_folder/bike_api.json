  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "ReserveRideDto": {
        "type": "object",
        "properties": {
          "bikeId": {
            "type": "number",
            "example": 1
          },
          "tariffPlanId": {
            "type": "number",
            "example": 1
          }
        },
        "required": ["bikeId"]
      },
      "City": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "name": {
            "type": "string",
            "example": "New York"
          },
          "countryCode": {
            "type": "string",
            "example": "US"
          },
          "timezone": {
            "type": "string",
            "example": "America/New_York"
          }
        },
        "required": ["id", "name", "countryCode", "timezone"]
      },
      "Bike": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "code": {
            "type": "string",
            "example": "BIKE-001"
          },
          "imei": {
            "type": "string",
            "example": "123456789012345",
            "nullable": true
          },
          "name": {
            "type": "string",
            "example": "Bike 001",
            "nullable": true
          },
          "status": {
            "type": "string",
            "enum": ["available", "in_use", "maintenance", "lost"],
            "example": "available"
          },
          "type": {
            "type": "string",
            "example": "standard",
            "nullable": true
          },
          "currentBatteryPercent": {
            "type": "number",
            "example": 85,
            "nullable": true
          },
          "batteryLevel": {
            "type": "number",
            "example": 85,
            "nullable": true
          },
          "internalBatteryLevel": {
            "type": "number",
            "example": 3.7,
            "nullable": true
          },
          "lastKnownLat": {
            "type": "number",
            "example": 40.7128,
            "nullable": true
          },
          "lastKnownLng": {
            "type": "number",
            "example": -74.006,
            "nullable": true
          },
          "latitude": {
            "type": "number",
            "example": 40.7128,
            "nullable": true
          },
          "longitude": {
            "type": "number",
            "example": -74.006,
            "nullable": true
          },
          "firmwareVersion": {
            "type": "string",
            "example": "V1.0.0",
            "nullable": true
          },
          "lastTelemetryAt": {
            "format": "date-time",
            "type": "string",
            "nullable": true
          },
          "lastSeenAt": {
            "format": "date-time",
            "type": "string",
            "nullable": true
          },
          "city": {
            "$ref": "#/components/schemas/City"
          },
          "cityId": {
            "type": "number",
            "nullable": true
          }
        },
        "required": [
          "id",
          "code",
          "imei",
          "name",
          "status",
          "type",
          "currentBatteryPercent",
          "batteryLevel",
          "internalBatteryLevel",
          "lastKnownLat",
          "lastKnownLng",
          "latitude",
          "longitude",
          "firmwareVersion",
          "lastTelemetryAt",
          "lastSeenAt",
          "city",
          "cityId"
        ]
      },
      "Tariff": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "name": {
            "type": "string",
            "example": "Standard Plan"
          },
          "type": {
            "type": "string",
            "enum": ["per_minute", "per_hour", "day_pass", "subscription"],
            "example": "per_minute"
          },
          "basePrice": {
            "type": "number",
            "example": 1
          },
          "pricePerMinute": {
            "type": "number",
            "example": 0.15
          },
          "minCharge": {
            "type": "number",
            "example": 2
          },
          "currency": {
            "type": "string",
            "example": "USD"
          },
          "activeFrom": {
            "format": "date-time",
            "type": "string",
            "nullable": true
          },
          "activeTo": {
            "format": "date-time",
            "type": "string",
            "nullable": true
          },
          "isActive": {
            "type": "boolean",
            "example": true
          }
        },
        "required": [
          "id",
          "name",
          "type",
          "basePrice",
          "pricePerMinute",
          "minCharge",
          "currency",
          "activeFrom",
          "activeTo",
          "isActive"
        ]
      },
      "Ride": {
        "type": "object",
